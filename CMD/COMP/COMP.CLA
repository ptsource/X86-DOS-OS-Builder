$M_A_00016H_END LABEL BYTE
  
$M_A_00017H_MSG LABEL BYTE
        DB      $M_A_00017H_END-$M_A_00017H_MSG-1
        DB      "Files compare OK",CR,LF
$M_A_00017H_END LABEL BYTE
  
