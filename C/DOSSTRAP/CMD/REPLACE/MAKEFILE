#************************** makefile for cmd\... ***************************

msg	=..\..\messages
dos	=..\..\dos
inc	=..\..\inc
hinc	=..\..\h

#
#######################	dependencies begin here. #########################
#

map	=..\..\mapper

all: replace.exe

_msgret.obj: _msgret.asm \
  $(inc)\msgserv.asm    \
  $(inc)\sysmsg.inc     \
  replace.ctl           \
  replace.cla           \
  replace.cl1           \
  replace.cl2           

_parse.obj: _parse.asm  \
  $(inc)\parse.asm      \
  $(inc)\psdata.inc

_replace.obj: _replace.asm

replace.obj: replace.c  \
  makefile           \
  replacep.h            \
  $(hinc)\comsub.h

replace.exe: replace.obj \
  _replace.obj          \
  $(map)\mapper.lib     \
  $(inc)\comsubs.lib    \
  _msgret.obj           \
  _parse.obj            \
  replace.lnk
    link @replace.lnk
    upx --8086 REPLACE.EXE
    del *.obj
